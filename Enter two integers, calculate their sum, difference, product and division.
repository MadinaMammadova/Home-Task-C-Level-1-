#include <iostream>
using namespace std;
int main() {
    int a,b,c,d,e,f;
    cout<<"Insert Number one = ";
    cin>>a;
    cout<<"Insert Number two = ";
    cin>>b;
    c=a+b;
    d=a-b;
    e=a*b;
    f=a/b;
    cout<<"Sum = "<<c<<endl;
    cout<<"Difference = "<<d<<endl;
    cout<<"Product = "<<e<<endl;
    cout<<"Division = "<<f<<endl;
    return 0;
}
